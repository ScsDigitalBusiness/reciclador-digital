<% if(errors.length > 0)  { %> 
    <% errors.forEach(err=>{ %> 
        <div class="alert alert-danger" role="alert">
            <%= err %>
        </div>
     <% }) %>
<% } %>

<% if(success.length > 0)  { %> 
    <% success.forEach(scs=>{ %> 
        <div class="alert alert-success" role="alert">
            <%= scs %>
        </div>
     <% }) %>
<% } %>

